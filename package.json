{
  "name": "trukapp",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:android": "expo start --android -c",
    "start:ios": "expo start --ios",
    "start:eas": "expo start --dev-client --port 8082",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas:build:android": "eas build -p android --profile production",
    "eas:build:dev": "eas build -p android --profile development",
    "build:android:apk": "eas build -p android --profile production-apk",
    "build:android:aab": "eas build -p android --profile production",
    "build:ios:appstore": "eas build -p ios --profile appstore",
    "build:preview": "eas build --profile preview",
    "build:prod:all": "npm run build:android:aab && npm run build:ios:appstore",
    "build:prod:full": "bash ./build.sh",
    "setup": "npm install && npx pod-install --silent || true",
    "lint": "expo lint",
    "postinstall": "patch-package"
  },
